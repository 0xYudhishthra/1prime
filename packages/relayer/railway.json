{
  "$schema": "https://railway.com/railway.schema.json",
  "build": {
    "nixpacksPlan": {
      "phases": {
        "install": {
          "dependsOn": ["setup"],
          "cmds": [
            "cd packages/cross-chain-sdk && pnpm install && pnpm build",
            "pnpm install --no-frozen-lockfile"
          ]
        }
      }
    }
  }
}
